{
  "name": "gitflow-init",
  "version": "0.0.0",
  "description": "Zero-config semantic-release, commitlint & Husky bootstrapper",
  "bin": {
    "gitflow-init": "./bin/gitflow-init"
  },
  "preferGlobal": true,
  "files": [
    "bin/",
    ".release-it.json",
    "commitlint.config.js",
    ".husky/"
  ],
  "scripts": {
    "prepare": "husky",
    "release": "dotenv -e .env -- release-it"
  },
  "keywords": [
    "git",
    "workflow",
    "commitlint",
    "husky",
    "release-it"
  ],
  "author": "arizmuajianisan",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "dotenv-cli": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4"
  }
}
